{
  "name": "@imagine-cx/imaginecx-library",
  "private": false,
  "version": "0.3.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && npm run build:style",
    "build:style": "postcss src/components/style.css -o ./dist/style.css",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{jsx,ts,tsx}'",
    "format": "prettier --write src//**/*.{ts,tsx,css} --config ./.prettierrc",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint",
      "npm run format"
    ]
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/imaginecx-library.es.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/imaginecx-library.umd.js"
      }
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./dist/*": "./dist/*.d.ts"
  },
  "main": "dist/imaginecx-library.umd.js",
  "module": "dist/imaginecx-library.es.js",
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "date-fns": "^2.29.3",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hooks": "^1.0.1",
    "tailwindcss": "^3.3.1",
    "typescript": "^4.9.5",
    "vite": "^4.2.0",
    "vite-plugin-dts": "^2.2.0",
    "vite-plugin-linter": "^2.0.2",
    "vite-tsconfig-paths": "^4.0.9"
  },
  "peerDependencies": {
    "date-fns": ">=2.29.3",
    "downshift": ">=8.2.3",
    "react": "16.8.0 || >=17.x",
    "react-dom": "16.8.0 || >=17.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Imagine-CX/imaginecx-library"
  },
  "keywords": [],
  "author": "Imagine CX",
  "bugs": {
    "url": "https://github.com/Imagine-CX/imaginecx-library/issues"
  },
  "homepage": "https://github.com/Imagine-CX/imaginecx-library#readme",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "clsx": "^1.2.1",
    "react-awesome-reveal": "^4.2.5",
    "react-icons": "^4.8.0",
    "react-router-dom": "^6.11.1"
  }
}
